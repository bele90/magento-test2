<?xml version="1.0"?>
<layout version="0.1.0">
    <bluecom_team_index_index>
        <reference name="content">
            <block type="bluecom_team/team" name="team" template="bluecom/team/team.phtml"/>
        </reference>
        <reference name="root">
            <action method="setTemplate"><template>page/2columns-right.phtml</template></action>
        </reference>
        <reference name="head">
            <action method="addItem"><type>skin_css</type><name>css/bluecom/team/vallenato.css</name></action>
            <action method="addJs"><script>lib/jquery/jquery-1.5.min.js</script></action>
            <action method="removeItem"><type>js</type><name>lib/jquery/jquery-1.10.2.min.js</name></action>
            <action method="addJs"><script>bluecom/team/vallenato.js</script></action>
        </reference>
    </bluecom_team_index_index>

</layout>
